---
import PageLayout from "../components/PageLayout.astro";
import bobawip from "../../../public/images/bobawip.png";
---

<PageLayout pageTitle="About Us">
  <h1>About us</h1>
  <p>
    We are a group of volunteers working to bring modern web development to
    fandom and other online communities.
  </p>
  <img src={bobawip} />
  <div>Under Construction</div>
</PageLayout>

<style>
  img {
    display: block;
    max-width: 100%;
  }
  div {
    text-align: center;
    font-size: x-large;
  }
</style>
